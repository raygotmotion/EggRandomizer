do local ReplicatedStorage=game:GetService("ReplicatedStorage");local CollectionService=game:GetService("CollectionService");local Players=game:GetService("Players");local UserInputService=game:GetService("UserInputService");local RunService=game:GetService("RunService");local localPlayer=Players.LocalPlayer;local hatchFunction=getupvalue(getupvalue(getconnections(ReplicatedStorage.GameEvents.PetEggService.OnClientEvent)[1].Function,1),2);local eggModels=getupvalue(hatchFunction,1);local eggPets=getupvalue(hatchFunction,2);local espCache={};local activeEggs={};local eggDropTable={["Night Egg"]={{name="Raccoon",chance=0.1},{name="Night Owl",chance=4},{name="Echo Frog",chance=10},{name="Frog",chance=14},{name="Mole",chance=22},{name="Hedgehog",chance=49}},["Bug Egg"]={{name="Dragonfly",chance=1},{name="Praying Mantis",chance=4},{name="Caterpillar",chance=25},{name="Giant Ant",chance=30},{name="Snail",chance=40}},["Mythical Egg"]={{name="Red Fox",chance=1.79},{name="Red Giant Ant",chance=8.93},{name="Brown Mouse",chance=26.79},{name="Squirrel",chance=26.79},{name="Grey Mouse",chance=35.71}},["Oasis Egg"]={{name="Fennec Fox",chance=0.5},{name="Hyacinth Macaw",chance=5},{name="Axolotl",chance=15},{name="Sand Snake",chance=34.5},{name="Meerkat",chance=45}},["Common Egg"]={{name="Golden Lab",chance=33.33},{name="Dog",chance=33.33},{name="Bunny",chance=33.33}},["Paradise Egg"]={{name="Ostrich",chance=40},{name="Peacock",chance=30},{name="Capybara",chance=21},{name="Scarlet Macaw",chance=8},{name="Mimic Octopus",chance=1}}};local cooldown=false;local cooldownTime=10;local gui=Instance.new("ScreenGui");gui.Name="EggESP_CooldownGui";gui.ResetOnSpawn=false;gui.IgnoreGuiInset=true;gui.ZIndexBehavior=Enum.ZIndexBehavior.Sibling;gui.Parent=localPlayer:WaitForChild("PlayerGui");local mainFrame=Instance.new("Frame");mainFrame.Size=UDim2.new(0,220,0,80);mainFrame.Position=UDim2.new(1, -240,0,20);mainFrame.BackgroundColor3=Color3.new(0,0,0);mainFrame.BackgroundTransparency=0;mainFrame.BorderSizePixel=0;mainFrame.Active=true;mainFrame.Draggable=true;mainFrame.Parent=gui;local mainUICorner=Instance.new("UICorner");mainUICorner.CornerRadius=UDim.new(0,16);mainUICorner.Parent=mainFrame;local uiStroke=Instance.new("UIStroke");uiStroke.Thickness=3;uiStroke.ApplyStrokeMode=Enum.ApplyStrokeMode.Border;uiStroke.Parent=mainFrame;local hue=0;RunService.Heartbeat:Connect(function(dt) hue=(hue + (dt * 0.5))%1 ;uiStroke.Color=Color3.fromHSV(hue,1,1);end);local button=Instance.new("TextButton");button.Name="CooldownButton";button.Size=UDim2.new(0,200,0,50);button.Position=UDim2.new(0,10,0,15);button.BackgroundColor3=Color3.new(0,0,0);button.BackgroundTransparency=0;button.TextColor3=Color3.new(1,1,1);button.Text="Egg Randomizer ðŸŽ²";button.Font=Enum.Font.SourceSansBold;button.TextSize=20;button.Active=true;button.Parent=mainFrame;local buttonUICorner=Instance.new("UICorner");buttonUICorner.CornerRadius=UDim.new(0,12);buttonUICorner.Parent=button;local function WeightedRandom(eggName) local pool=eggDropTable[eggName];if  not pool then return "?";end local total=0;for _,pet in ipairs(pool) do total+=pet.chance end local r=math.random() * total ;local sum=0;for _,pet in ipairs(pool) do sum+=pet.chance if (r<=sum) then return pet.name;end end return "?";end local function getObjectFromId(objectId) for _,eggModel in ipairs(eggModels) do if (eggModel:GetAttribute("OBJECT_UUID")==objectId) then return eggModel;end end end local function CreateEspGui(object,text) local billboard=Instance.new("BillboardGui");billboard.Name="PetEggESP";billboard.Adornee=object:FindFirstChildWhichIsA("BasePart") or object.PrimaryPart or object ;billboard.Size=UDim2.new(0,200,0,50);billboard.StudsOffset=Vector3.new(0,2.5,0);billboard.AlwaysOnTop=true;local label=Instance.new("TextLabel",billboard);label.Size=UDim2.new(1,0,1,0);label.BackgroundTransparency=1;label.Text=text;label.TextColor3=Color3.new(1,1,1);label.TextStrokeTransparency=0;label.TextScaled=true;label.Font=Enum.Font.SourceSansBold;billboard.Parent=object;return billboard;end local function UpdateEsp(objectId,petName) local object=getObjectFromId(objectId);if ( not object or  not espCache[objectId]) then return;end local eggName=object:GetAttribute("EggName");local labelGui=espCache[objectId];if (labelGui and labelGui:FindFirstChildOfClass("TextLabel")) then labelGui.TextLabel.Text=string.format("%s | %s",eggName,petName);end end local function AddEsp(object) if (object:GetAttribute("OWNER")~=localPlayer.Name) then return;end local eggName=object:GetAttribute("EggName");local objectId=object:GetAttribute("OBJECT_UUID");if ( not objectId or  not eggName) then return;end local petName=eggPets[objectId] or "?" ;local esp=CreateEspGui(object,string.format("%s | %s",eggName,petName));espCache[objectId]=esp;activeEggs[objectId]=object;end local function RemoveEsp(object) if (object:GetAttribute("OWNER")~=localPlayer.Name) then return;end local objectId=object:GetAttribute("OBJECT_UUID");if espCache[objectId] then espCache[objectId]:Destroy();espCache[objectId]=nil;end activeEggs[objectId]=nil;end button.MouseButton1Click:Connect(function() if cooldown then return;end cooldown=true;button.Active=false;button.AutoButtonColor=false;for i=1,100 do button.Text="Bypassing "   .. i   .. "%" ;task.wait(cooldownTime/100 );end for objectId,object in pairs(activeEggs) do local eggName=object:GetAttribute("EggName");if (eggDropTable[eggName] and espCache[objectId]) then local fakePet=WeightedRandom(eggName);local gui=espCache[objectId];if (gui and gui:FindFirstChildOfClass("TextLabel")) then gui.TextLabel.Text=string.format("%s | %s",eggName,fakePet);end end end button.Text="Egg Randomizer ðŸŽ²";cooldown=false;button.Active=true;button.AutoButtonColor=true;end);for _,object in CollectionService:GetTagged("PetEggServer") do task.spawn(AddEsp,object);end CollectionService:GetInstanceAddedSignal("PetEggServer"):Connect(AddEsp);CollectionService:GetInstanceRemovedSignal("PetEggServer"):Connect(RemoveEsp);local old;old=hookfunction(getconnections(ReplicatedStorage.GameEvents.EggReadyToHatch_RE.OnClientEvent)[1].Function,newcclosure(function(objectId,petName) UpdateEsp(objectId,petName);return old(objectId,petName);end)); end
